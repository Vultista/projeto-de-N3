        if(is404page()) return;
        waitFor(getRequiredElements)
          .then(function(subscription) {
            getCampaign(CAMPAIGN_NAME, subscription);
          })
          .catch(function() {
            // silent
          });
        
            function is404page(){
    return document.querySelector('meta[name="searchtitle"]')?.getAttribute('content') === '404';
  }

        function getRequiredElements() {
          return isMarketTranslated() && isSignedInOrHidden();
        }

        function isMarketTranslated() {
          return MARKETS[MARKET];
        }

        function isSignedInOrHidden() {
          return !isUserLoggedIn() && !isCookieLogin() && !isHiddenPopupForThisSession();
        }

        function isUserLoggedIn() {
          return window.digitalData &&
            window.digitalData.user &&
            window.digitalData.user.profile &&
            window.digitalData.user.profile.profileInfo &&
            window.digitalData.user.profile.profileInfo.attributes &&
            window.digitalData.user.profile.profileInfo.attributes.visitorType &&
            window.digitalData.user.profile.profileInfo.attributes.visitorType === 'signed_in';
        }

        function isCookieLogin() {
          try {
            return getCookie('isSignedIn') === 'true';
          } catch (err) {
            return null;
          }
        }

        function getCookie(name) {
          var v = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
          return v ? v[2] : null;
        }

        function isHiddenPopupForThisSession() {
          return window.sessionStorage.getItem(CAMPAIGN_NAME);
        }


        function getCampaign(activityMbox) {
          var getOffer = window.adobe.target.getOffer;
          var applyOffer = window.adobe.target.applyOffer;
          window.lc = window.lc || {};
          window.lc.plays536 = MARKETS[MARKET];
          getOffer({
            mbox: activityMbox,
            params: {
              at_property: '64fb86e5-6320-8bbd-a4ac-de1b2e6d014d',
            },
            success: function(offer) {
              applyOffer({
                mbox: activityMbox,
                offer: offer
              });
            },
            error: function(e) {
              // eslint-disable-next-line no-console
              console.log(e);
            }
          });
        }

        function waitFor(condition, timeout) {
          var stopTime = typeof timeout === 'number' ? timeout : 7000;
          var stop = false;

          window.setTimeout(function() {
            stop = true;
          }, stopTime);

          return new Promise(function(resolve, reject) {
            (function _innerWaitFor() {
              var value = condition();

              if (stop) {
                reject();
              } else if (value) {
                resolve(value);
              } else {
                window.setTimeout(_innerWaitFor, 50);
              }
            })();
          });
        }
        // eslint-disable-next-line no-undef
      })();
    } catch (err) {
      console.log('LC:', err);
    }
  })('');
  //# sourceURL=lc/targeting/lc-targeting.js
