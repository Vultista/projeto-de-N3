      try {
        waitFor(getProfileData, init)();
      } catch (error) {
        // silent
      }
      function getProfileData() {
        let user = null;
        try {
          user = window.digitalData.user.profile.profileInfo.attributes;
        } catch (e) {
          /*pass*/
        }
        return user;
      }
  
      function init(profileAttrs) {
        if (!window.sessionStorage.getItem("lc-prompt-closed")) {
          return;
        }
        if (
          profileAttrs.visitorType === "signed_in" ||
          getCookie("isSignedIn") === "true"
        ) {
          fetch(APOLLO_CARTCOUNT_ENDPOINT, {
            headers: {
              accept: "application/json",
              "accept-language": "en-GB,en-US;q=0.9,en;q=0.8",
              "apollographql-client-name": "oracle-web-toolbar",
              "apollographql-client-version": "1.5.1",
              "content-type": "application/json",
            },
            referrerPolicy: "strict-origin-when-cross-origin",
            body: null,
            method: "GET",
            mode: "cors",
            credentials: "include",
          })
            .then((r) => r.json())
            .then((r) => {
              let itemCount = 0;
              try {
                itemCount = r.data.webCheckoutCartRetrieve.itemCount;
              } catch (e) {
                /**/
              }
              if (itemCount > 0) {
                getCampaign("plays1376");
              }
            });
        }
      }
  
      function getCookie(name) {
        var v = document.cookie.match("(^|;) ?" + name + "=([^;]*)(;|$)");
        return v ? v[2] : null;
      }
  
      function getCampaign(activityMbox) {
        var getOffer = window.adobe.target.getOffer;
        var applyOffer = window.adobe.target.applyOffer;
  
        getOffer({
          mbox: activityMbox,
          params: {
            at_property: "64fb86e5-6320-8bbd-a4ac-de1b2e6d014d",
          },
          success: function (offer) {
            applyOffer({
              mbox: activityMbox,
              offer: offer,
            });
          },
          error: function (status, error) {
            // eslint-disable-next-line no-console
            console.log("Error", status, error);
          },
        });
      }
  
      function waitFor(condition, callback, timeout) {
        var stopTime = typeof timeout === "number" ? timeout : 7000;
        var stop = false;
  
        window.setTimeout(function () {
          stop = true;
        }, stopTime);
  
        return function _innerWaitFor() {
          var value = condition();
          if (stop) return null;
          if (value) return callback(value);
          return window.setTimeout(_innerWaitFor, 50);
        };
      }
    } catch (err) {
      console.log("LC:", err);
    }
  })();
    </script><script>
  (function() {
    function getCookie(name) {
      let match = document.cookie.match(new RegExp('(^|;|;\\s)' + name + '=([^;]+)'));
      return match ? match[2] : '';
    }
  
    function when(conditionFn, stopAfter = 7000) {
      return new Promise((resolve, reject) => {
        let stopByTimeout = false;
        let value;
        window.setTimeout(() => {
          stopByTimeout = true;
        }, stopAfter);
        (function _innerWaitFor() {
          if (stopByTimeout) {
            reject(Error('when stopped: by timeout'));
            return;
          }
          try {
            value = conditionFn();
          } catch (e) {/* silent */}
          value ? resolve(value) : window.setTimeout(_innerWaitFor, 50);
        })();
      });
    }
  
    function isInIframe(base = window) {
      try {
        return base.self !== base.parent;
      } catch (e) {
        return true;
      }
    }
  
    function getLogger(prefix, cookieName, cookieValue) {
      const prefixes = _toArray(prefix);
      const result = {
        ...getLoggerObject({
          prefixes,
          cookieName
        }),
        debug: getLoggerObject({
          prefixes: [...prefixes, 'debug'],
          cookieName,
          cookieValue,
          style: 'background-color: #E0E0E0; display: inline-block; padding: 2px 0 2px 0; border-radius: 2px;'
        })
      };
      return result;
    }
    function getLoggerObject(settings) {
      const typesMap = {
        info: 'blue',
        log: 'blue',
        ok: 'green',
        error: 'crimson',
        warn: '#f9a66d'
      };
  
      const result = {};
      Object.keys(typesMap).forEach(method => {
        result[method] = getLoggingMethod({
          ...settings,
          color: typesMap[method]
        });
      });
      return result;
    }
    function getLoggingMethod(settings) {
      const CONSOLE_METHOD = 'info';
      const {
        cookieName,
        cookieValue,
        color,
        style,
        prefixes
      } = settings;
      return function () {
        if (!_isEnabled(cookieName, cookieValue)) return;
        const iframePrefix = isInIframe() ? 'iframe' : '';
        const allPrefixes = [iframePrefix, ...prefixes].filter(Boolean);
        const args = [];
        [...arguments].forEach(arg => {
          if (typeof arg === 'string' && arg.startsWith('[p]')) {
            allPrefixes.push(arg.slice(3));
          } else {
            args.push(arg);
          }
        });
        if (window.console && typeof window.console[CONSOLE_METHOD] === 'function') {
          window.console[CONSOLE_METHOD]('%c' + allPrefixes.map(n => '[' + n + ']').join('') + ':', 'color: ' + color + '; ' + (style || ''), ...args);
        }
      };
    }
  
    function _isEnabled(cookieName, cookieValue) {
      const currentCookieValue = getCookie(cookieName);
      if (typeof cookieValue === 'undefined') return Boolean(currentCookieValue);
      return currentCookieValue === cookieValue.toString();
    }
    function _toArray(item) {
      return (Array.isArray(item) ? item : [item]).filter(Boolean);
    }
  
    const CAMPAIGN_ID = 'c-plays-1372-redirect-user-to-notification';
    const CAMPAIGN_PREFIX = 'lc-c-plays-1372-redirect-user-to-notification';
  
    function getCampaign() {
      let getOffer = window.adobe.target.getOffer;
      let applyOffer = window.adobe.target.applyOffer;
      getOffer({
        mbox: CAMPAIGN_ID,
        params: {
          at_property: '64fb86e5-6320-8bbd-a4ac-de1b2e6d014d'
        },
        success: function (offer) {
          applyOffer({
            mbox: CAMPAIGN_ID,
            offer: offer
          });
        },
        error: function (status, error) {
        // eslint-disable-next-line no-console
          console.log('Error', status, error);
        }
      });
    }
  
    const infoLogger = getLogger(CAMPAIGN_PREFIX + '-info', 'lc-debug', CAMPAIGN_ID);
    function dependenciesReady() {
      return isjQueryAvailable();
    }
    function isjQueryAvailable() {
      const available = typeof jQuery !== 'undefined';
      return available;
    }
    function isUserLoggedOut() {
      const visitorType = window.digitalData?.user?.profile?.profileInfo?.attributes?.visitorType;
      const loggedOut = visitorType !== 'signed_in';
      return loggedOut;
    }
    function isCampaignClosed() {
  const closedInSession = localStorage.getItem('-popup-closed') === 'true'; 
  return closedInSession;
}
    when(dependenciesReady).then(initTargeting).catch(() => {
      infoLogger.info('targeting dependencies not found');
    });
    function initTargeting() {
      const userLoggedOut = isUserLoggedOut();
      const campaignClosed = isCampaignClosed();
      if (!userLoggedOut && !campaignClosed) {
        generateCampaign();
      } else {
        handleFail('User is logged in, incorrect page, or campaign is closed');
      }
    }
    function generateCampaign() {
      getCampaign();
      let event = new CustomEvent('targeting:generated');
      window.dispatchEvent(event);
    }
    function handleFail(targetingFailed) {
      infoLogger.info('activation failed for -> targeting - ' + targetingFailed);
    }
  })();
  //# sourceURL=url://LeanConvert/c-plays-1372-redirect-user-to-notification/targeting/targeting.html
</script><style>

</style><script>
  ;
  (function(html) {
    try {
      var COOKIE_NAME = 'lc-a40-status';
      var COOKIE_TIME = 30; // days

      waitFor(getProfileData, init)();

      function getProfileData() {
        return window.digitalData &&
          window.digitalData.user &&
          window.digitalData.user.profile &&
          window.digitalData.user.profile.profileInfo &&
          window.digitalData.user.profile.profileInfo.attributes;
      }

      function init(profileAttrs) {
        var visitorType = profileAttrs.visitorType;
        var psplusStatus = profileAttrs.plusUser;

        if ((visitorType === 'signed_in' || getCookie('isSignedIn') === 'true') && (psplusStatus !== null)) {
          setCookie(
            COOKIE_NAME,
            'psplus:' + psplusStatus,
            COOKIE_TIME
          );
        }
      }

      function setCookie(name, value, days) {
        var date;
        var expires;

        if (days) {
          date = new Date();
          date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
          expires = '; expires=' + date.toGMTString();
        } else {
          expires = '';
        }
        document.cookie = name + '=' + value + expires + '; path=/';
      }

      // eslint-disable-next-line no-unused-vars
      function getCookie(name) {
        var v = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
        return v ? v[2] : null;
      }

      function waitFor(condition, callback, timeout) {
        var stopTime = typeof timeout === 'number' ? timeout : 7000;
        var stop = false;

        window.setTimeout(function() {
          stop = true;
        }, stopTime);

        return function _innerWaitFor() {
          var value = condition();

          if (stop) return null;

          if (value) return callback(value);

          return window.setTimeout(_innerWaitFor, 50);
        };
      }

    } catch (err) {
      console.log('LC:', err);
    }
  })('\n');
  //# sourceURL=a40-psplus-cookie.js
</script><style>
  /*# sourceURL=lc/plays423/lc-plays423-monitoring-script.css */
</style><script>
  ;
  (function(html) {
    try {
      (function() {
        var STORAGE_ENTRY = 'lc-cart-state';
        var SINGLE_USER_STORAGE_LIMIT = 50;

        addMessageListener();

        function addMessageListener() {
          window.addEventListener('message', function(event) {
            var eventData = {};
            try {
              eventData = JSON.parse(event.data);
            } catch (e) {
              return;
            }

            var isAddToCart = (eventData.action === 'checkout_item_added');
            var isRemoveFromCart = (eventData.action === 'checkout_item_removed');
            var isPurchaseComplete = (eventData.action === 'checkout_purchase_complete');
            if (!isAddToCart && !isRemoveFromCart && !isPurchaseComplete) return;

            var productID = (eventData.data && eventData.data.skuId);
            if (!productID && (isAddToCart || isRemoveFromCart)) return;

            var profileID = getProfileID();
            if (!profileID) return;

            if (isAddToCart) {
              // console.log(SCRIPT_ID, ' PRODUCT ADDED: ', productID);
              saveCartEntry(profileID, productID, (new Date()).getTime());
            }

            if (isRemoveFromCart) {
              // console.log(SCRIPT_ID, ' PRODUCT REMOVED: ', productID);
              removeCartEntry(profileID, productID);
            }

            if (isPurchaseComplete) {
              // console.log(SCRIPT_ID, ' PURCHASE COMPLETE');
              removeUserEntry(profileID);
            }
          });
        }

        function removeCartEntry(profileID, productID) {
          var state = getCartState();
          var userEntryArr = normalizeUserEntryArr(state[profileID] || []);
          if (!userEntryArr.length) return;

          var existingProductEntry = userEntryArr.find(function(productEntryObj) {
            return (productEntryObj.productID === productID);
          });
          if (existingProductEntry) {
            userEntryArr.splice(userEntryArr.indexOf(existingProductEntry), 1);
            state[profileID] = userEntryArr;
            saveCartState(state);
          }
        }

        function saveCartEntry(profileID, productID, timeStamp) {
          var state = getCartState();
          var userEntryArr = normalizeUserEntryArr(state[profileID] || []);
          var existingProductEntry = userEntryArr.find(function(productEntryObj) {
            return (productEntryObj.productID === productID);
          });

          if (existingProductEntry) {
            existingProductEntry.timeStamp = timeStamp;
          } else {
            userEntryArr.unshift({
              productID: productID,
              timeStamp: timeStamp
            });
          }

          if (userEntryArr.length > SINGLE_USER_STORAGE_LIMIT) {
            userEntryArr.length = SINGLE_USER_STORAGE_LIMIT;
          }

          state[profileID] = userEntryArr;
          saveCartState(state);
        }

        function removeUserEntry(profileID) {
          var state = getCartState();
          delete state[profileID];
          saveCartState(state);
        }

        function normalizeUserEntryArr(userEntryArr) {
          return userEntryArr.filter(function(productEntryObj) {
            return (
              typeof productEntryObj === 'object' &&
              productEntryObj &&
              productEntryObj.productID &&
              productEntryObj.timeStamp
            );
          });
        }

        function getCartState() {
          var state = {};
          try {
            state = JSON.parse(window.localStorage.getItem(STORAGE_ENTRY)) || {};
          } catch (e) {
            /* silent */ }

          return state;
        }

        function saveCartState(state) {
          window.localStorage.setItem(STORAGE_ENTRY, JSON.stringify(state));
        }

        function getProfileID() {
          var profileNameContainer = document.querySelector('.psw-player-name[data-qa*="playerName2"] .psw-player-name-text');
          var profileName = profileNameContainer && profileNameContainer.innerText;

          return (profileName || '').trim();
        }
      })();

    } catch (err) {
      console.log('LC:', err);
    }
  })('');
  //# sourceURL=lc/plays423/lc-plays423-monitoring-script.js
</script><style>
  #js-SIEWS1lib-header-Base {
    display: none !important;
  }

