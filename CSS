</style><script>
  ;
  (function(html) {
    try {
      (function() {
        var COOKIE_NAME = 'lc-plays484';
        var PSSTARS_QUERY = '$ROOT_QUERY.loyaltyAccountRetrieve';
        var USERID_QUERY = '$ROOT_QUERY.oracleUserProfileRetrieve';
        var COOKIE_TIME = 30; // days

        waitFor(function() {
          return getUserId() && getPsStarsStatus() && getProfileData();
        }, init)();

        function getProfileData() {
          return window.digitalData &&
            window.digitalData.user &&
            window.digitalData.user.profile &&
            window.digitalData.user.profile.profileInfo &&
            window.digitalData.user.profile.profileInfo.attributes;
        }

        function getPsStarsStatus() {
          return getData(PSSTARS_QUERY + '({"accountId":"' + getUserId() + '"}).enrollStatus');
        }

        function getUserId() {
          return getData(USERID_QUERY) && getData(USERID_QUERY).accountId;
        }

        function getData(query) {
          return window.__APOLLO_CLIENT__ &&
            window.__APOLLO_CLIENT__.cache &&
            window.__APOLLO_CLIENT__.cache.data &&
            window.__APOLLO_CLIENT__.cache.data.data &&
            window.__APOLLO_CLIENT__.cache.data.data[query];
        }

        function init(profileAttrs) {
          try {
            $('body').addClass(COOKIE_NAME);
            var signedIn = (profileAttrs.visitorType === 'signed_in' || getCookie('isSignedIn') === 'true') || false;
            var psStarsCookieValue = getPsStarsStatus() && getPsStarsStatus().isUserEnrolled;

            if (signedIn) {
              setCookie(
                COOKIE_NAME,
                JSON.stringify(psStarsCookieValue),
                COOKIE_TIME
              );
            }
          } catch (error) {
            // silent
          }
        }
        
        function getCookie(name) {
  var v = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
  return v ? v[2] : null;
}

        function setCookie(name, value, days) {
          var date;
          var expires;

          if (days) {
            date = new Date();
            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
            expires = '; expires=' + date.toGMTString();
          } else {
            expires = '';
          }
          document.cookie = name + '=' + value + expires + '; path=/';
        }

        function waitFor(condition, callback, timeout) {
          var stopTime = typeof timeout === 'number' ? timeout : 7000;
          var stop = false;

          window.setTimeout(function() {
            stop = true;
          }, stopTime);

          return function _innerWaitFor() {
            var value = condition();

            if (stop) return null;

            if (value) return callback(value);

            return window.setTimeout(_innerWaitFor, 50);
          };
        }
      })();

    } catch (err) {
      console.log('LC:', err);
    }
  })('');
  //# sourceURL=lc/cookie/lc-cookie.js
</script><style>
  /*# sourceURL=lc/cookie/lc-cookie.css */
</style><script>
  ;
  (function(html) {
    try {
      (function() {
        var COOKIE_NAME = 'lc-plays410';
        var PSPLUS_SUBSCRIPTION_TYPE = 'PSPLUS';
        var COOKIE_TIME = 30; // days
        var TIERS = {
          TIER_30: 'Deluxe',
          TIER_20: 'Extra',
          TIER_10: 'Essential',
          NONE: false
        };

        var plays410Storage = {
          getLocalStorage: function() {
            return localStorage.getItem(COOKIE_NAME);
          },
          isLocalStorageEmpty: function() {
            return localStorage.getItem(COOKIE_NAME) === null;
          },
          setLocalStorage: function(obj) {
            localStorage.setItem(COOKIE_NAME, JSON.stringify(obj));
          },
          addPSCookie: function(obj) {
            setCookie(COOKIE_NAME, JSON.stringify(obj), COOKIE_TIME);
          },
          getPSCookie: function() {
            return getCookie(COOKIE_NAME);
          }
        };

        try {
          waitFor(function() {
            return getPsPlusData() && getProfileData();
            // return getDataForTiers(PSPLUS_COOKIE_QUERY) && getProfileData();
          }, init)();
          waitFor(function() {
            return !plays410Storage.isLocalStorageEmpty() && !plays410Storage.getPSCookie();
          }, function() {
            plays410Storage.addPSCookie(JSON.parse(plays410Storage.getLocalStorage()));
          }, 4000)();
        } catch (err) {
          // silent
        }

        function getProfileData() {
          try {
            return window.digitalData.user.profile.profileInfo.attributes;
          } catch (err) {
            return null;
          }
        }

        function getPsPlusData() {
          try {
            return Object.values(window.__APOLLO_CLIENT__.cache.data.data).filter(data => data.subscriptionType === PSPLUS_SUBSCRIPTION_TYPE)[0]
          } catch (err) {
            return null;
          }
        }

        function init(profileAttrs) {
          try {
            var signedIn = (profileAttrs.visitorType === 'signed_in' || getCookie('isSignedIn') === 'true') || false;
            var subscriptionValue = getSubscriptionValue(getPsPlusData()) || false;

            if (signedIn && subscriptionValue) {
              plays410Storage.setLocalStorage(subscriptionValue);
              plays410Storage.addPSCookie(subscriptionValue);
            }
          } catch (error) {
            // silent
          }
        }

        function getSubscriptionValue(subscription) {
          return {
            [subscription.subscriptionType]: {
              duration: subscription.subscriptionDuration || false,
              isSubscribed: subscription.subscriptionStatus === 'SUBSCRIBED' || false,
              tier: getTier(subscription.subscriptionTier) || false,
              type: subscription.subscriptionType || false
            }
          };
        }

        function getTier(rawTier) {
          return TIERS[rawTier];
        }

        function setCookie(name, value, days) {
          var date;
          var expires;

          if (days) {
            date = new Date();
            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
            expires = '; expires=' + date.toGMTString();
          } else {
            expires = '';
          }
          document.cookie = name + '=' + value + expires + '; path=/';
        }

        // eslint-disable-next-line no-unused-vars
        function getCookie(name) {
          var v = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
          return v ? v[2] : null;
        }

        function waitFor(condition, callback, timeout) {
          var stopTime = typeof timeout === 'number' ? timeout : 7000;
          var stop = false;

          window.setTimeout(function() {
            stop = true;
          }, stopTime);

          return function _innerWaitFor() {
            var value = condition();

            if (stop) return null;

            if (value) return callback(value);

            return window.setTimeout(_innerWaitFor, 50);
          };
        }
      })();

    } catch (err) {
      console.log('LC:', err);
    }
  })('');
  //# sourceURL=lc/cookie/lc-cookie.js
</script><style>
