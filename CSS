    (function () {
    var APOLLO_CARTCOUNT_ENDPOINT =
      "https://web.np.playstation.com/api/graphql/v1/op?operationName=getCartItemCount&variables=%7B%7D&extensions=%7B%22persistedQuery%22%3A%7B%22version%22%3A1%2C%22sha256Hash%22%3A%2298136bcbc72e0fefccd8ecd6d3b3309225a6889c19df6e54581d86ff1c15d88a%22%7D%7D";
